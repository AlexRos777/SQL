1.Создать таблицу employees 
	- id. serial, primary key,
	- employee_name. 
        - Varchar(50), not null

CREATE TABLE employees_77 (
	id SERIAL PRIMARY KEY,
	employee_name VARCHAR(50) NOT NULL
);
SELECT * FROM employees_77;

